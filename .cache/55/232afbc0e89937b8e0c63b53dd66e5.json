nbody {\n  height: 100vh;\n  width: 100%;\n  background: var(--main-green);\n}\n\nbutton {\n  height: 100%;\n  border: none;\n  border-radius: 3px;\n  outline: none;\n  background: var(--main-blue);\n  color: var(--main-white);\n  cursor: pointer;\n}\n\n.wrapper {\n  margin: 120px auto;\n  max-width: 400px;\n  width: 100%;\n  border-radius: 5px;\n  background: var(--main-white);\n  padding: 25px;\n}\n\n.wrapper header {\n  font-size: 30px;\n  font-weight: 600;\n}\n\n.wrapper .inputField {\n  display: flex;\n  height: 45px;\n  width: 100%;\n  margin: 20px 0;\n}\n\n.inputField input {\n  width: 85%;\n  height: 100%;\n  border: 1px solid var(--sub-grey);\n  font-size: 17px;\n  border-radius: 3px;\n  padding-left: 15px;\n}\n\n.inputField button {\n  width: 45px;\n  margin-left: 10px;\n  font-size: 22px;\n}\n\n.wrapper .todoList {\n  max-height: 250px;\n  overflow-y: scroll;\n}\n\n.categoryWrap{\n  display: flex;\n  flex-wrap: wrap;\n  justify-content: space-between;\n}\n\n.todoList::-webkit-scrollbar {\n  width: 0;\n}\n.todoList li {\n  list-style: none;\n  height: 45px;\n  line-height: 45px;\n  position: relative;\n  background: var(--main-grey);\n  margin-bottom: 8px;\n  border-radius: 3px;\n  padding-left: 15px;\n  cursor: default;\n  overflow: hidden;\n}\n\n.done {\n  text-decoration: line-through;\n}\n\n.todoList li span {\n  position: absolute;\n  right: -45px;\n  width: 45px;\n  color: var(--main-white);\n  text-align: center;\n  background: var(--main-red);\n  border-radius: 0 3px 3px 0;\n  cursor: pointer;\n  transition: all 0.3s ease;\n}\n\n.todoList li span.editBtn {\n  position: absolute;\n  right: -45px;\n  width: 45px;\n  color: var(--main-white);\n  text-align: center;\n  background: var(--yellow);\n\n  cursor: pointer;\n  transition: all 0.3s ease;\n}\n\n.todoList li:hover span {\n  right: 0;\n}\n\n.wrapper .footer {\n  /*   width: 100%; */\n  display: flex;\n  justify-content: space-between;\n  margin-top: 20px;\n  align-items: center;\n}\n\n.footer button {\n  font-size: 16px;\n  padding: 6px 10px;\n}\n#viewBtn{\n  padding: .2rem;\n  height: fit-content;\n}\n\n#catEditPic{\n  transition: all .2s ease-in-out;\n}\n\n#catEditPic:hover{\n  transform: scale(1.1);\n  color:#464646;\n}\n\n/* MODAL CSS START */\n/* The Modal (background) */\n.modal {\n  display: none; /* Hidden by default */\n  position: fixed; /* Stay in place */\n  z-index: 1; /* Sit on top */\n  left: 0;\n  top: 0;\n  width: 100%; /* Full width */\n  height: 100%; /* Full height */\n  overflow: auto; /* Enable scroll if needed */\n  background-color: rgb(0,0,0); /* Fallback color */\n  background-color: rgba(0,0,0,0.4); /* Black w/ opacity */\n}\n\n/* Modal Content/Box */\n.modal-content {\n  background-color: #fefefe;\n  margin: 15% auto; /* 15% from the top and centered */\n  padding: 20px;\n  border: 1px solid #888;\n  width: 80%; /* Could be more or less, depending on screen size */\n}\n\n/* The Close Button */\n.close {\n  color: #aaa;\n  float: right;\n  font-size: 28px;\n  font-weight: bold;\n}\n\n.close:hover,\n.close:focus {\n  color: black;\n  text-decoration: none;\n  cursor: pointer;\n}\n\n/* MODAL CSS END */\n"},"lineCount":198}},"error":null,"hash":"1869ab6d4300447c6cb0eb30cee0cdf0","cacheData":{"env":{}}}